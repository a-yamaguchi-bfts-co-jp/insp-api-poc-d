# API Base URL
REACT_APP_API_BASE_URL=https://your-api-app.azurewebsites.net

# Azure AD Authentication
REACT_APP_AUTH_AUTHORITY=https://login.microsoftonline.com/your-tenant-id/v2.0
REACT_APP_AUTH_CLIENT_ID=api://your-app-name

# Authentication Bypass (開発・検証用)
# true: 認証をバイパス, false: 認証を有効化
REACT_APP_BYPASS_AUTH=true

# Environment
# development, staging, production
REACT_APP_ENVIRONMENT=development
